  "repository": {
    "type": "git",
    "url": "git+https://github.com/warp-exchange/rest-api.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -w"
