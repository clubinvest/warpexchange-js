{
  "name": "warpexchange",
  "description": "WarpExchange Payment Gateway :) ",
  "version": "1.0.7",
  "author": "lucasschoch",
  "bugs": {
    "url": "https://github.com/warp-exchange/rest-api/issues"
  },
  "dependencies": {
    "axios": "0.18.0"
  },
  "devDependencies": {
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.2",
    "tslint": "5.11.0",
    "typescript": "3.1.6"
  },
  "homepage": "https://github.com/warp-exchange/rest-api#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "bitcoin",
    "criptocurrencies",
    "gateway",
    "litecoin",
    "payment",
    "warp",
